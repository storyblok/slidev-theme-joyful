{
  "name": "slidev-theme-joyful",
  "type": "module",
  "version": "0.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "release": "release-it",
    "screenshot": "slidev export example.md --format png",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@slidev/types": "^52.1.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@slidev/cli": "^52.1.0",
    "@storyblok/eslint-config": "^0.4.1",
    "release-it": "^19.0.4",
    "vite-svg-loader": "^5.1.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "fonts": {
        "sans": "Roboto",
        "mono": "Fira Code"
      }
    }
  }
}
